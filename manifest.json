{
    "name": "WinCatch - to Bookmark Current Window",
    "version": "1.0",
    "description": "Bookmark all the open tabs in the current window!",
    "permissions": [
        "bookmarks",
        "tabs"
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "icons": {
        "128": "icon_192.png"
    },
    "manifest_version": 2
}